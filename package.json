    "axios": "^1.2.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dom": "0.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.0",
    "react": "^18.2.0",
    "router": "^1.3.7"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
